{
  "name": "go3",
  "version": "0.0.0",
  "scripts": {
    "build:frontend": "webpack",
    "build:server": "tsc -p . --outDir server/",
    "build:frontend:release": "webpack --config webpack.config-release.js",
    "start:frontend": "webpack-dev-server",
    "start:server": "ts-node-dev src/server",
    "lint:src": "tslint --format stylish --type-check --project tsconfig.json --exclude generated/**/*.ts",
    "lint:style": "stylelint --allow-empty-input src/**/*.scss",
    "test": "jest"
  },
  "main": "src/index.tsx",
  "repository": "https://github.com/Prior99/go3",
  "author": "Frederick Gnodtke",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(scss)$": "<rootDir>/jest-scss-preprocessor.js",
      ".(ts|tsx)": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setups.js",
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.0.39",
    "@types/morgan": "^1.7.35",
    "@types/recharts": "^1.0.5",
    "@types/yamljs": "^0.2.30",
    "bind-decorator": "^1.0.11",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "http-status-codes": "^1.3.0",
    "hyrest": "0.7.3",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "morgan": "^1.9.0",
    "path-to-regexp": "^2.1.0",
    "pg": "^7.4.0",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "reflect-metadata": "^0.1.10",
    "semantic-ui-react": "^0.77.0",
    "tsdi": "^0.14.0",
    "typeorm": "0.1.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.3",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/http-status-codes": "^1.0.29",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/ramda": "^0.24.3",
    "@types/react": "^15.0.37",
    "@types/react-dom": "^15.5.1",
    "@types/react-router-dom": "^4.0.7",
    "awesome-typescript-loader": "^3.2.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "git-revision-webpack-plugin": "^2.5.1",
    "jest": "^21.2.1",
    "mobx-react-devtools": "^4.2.15",
    "node-sass": "^4.5.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^7.13.0",
    "ts-jest": "^21.2.4",
    "ts-node-dev": "^1.0.0-pre.5",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.9.1",
    "yamljs": "^0.3.0"
  }
