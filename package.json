{
  "name": "go3",
  "version": "0.0.0",
  "scripts": {
    "build:web": "webpack",
    "build:server": "tsc -p . --outDir server/",
    "build:web:release": "webpack --config webpack.config-release.js",
    "start:web": "webpack-dev-server",
    "start:server": "TZ=UTC ts-node-dev src/server",
    "start:android": "react-native run-android",
    "start:native": "react-native start",
    "lint:src": "tslint --format stylish --type-check --project tsconfig.json --exclude generated/**/*.ts",
    "lint:style": "stylelint --allow-empty-input src/**/*.scss",
    "test": "TZ=UTC jest"
  },
  "main": "src/index.tsx",
  "repository": "https://github.com/Prior99/go3",
  "author": "Frederick Gnodtke",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(scss)$": "<rootDir>/jest-scss-preprocessor.js",
      ".(ts|tsx)": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setups.js",
    "testMatch": [
      "**/__tests__/test-*.(ts|tsx|js)"
    ],
    "mapCoverage": true
  },
